{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "edwiser-bridge/orders",
	"version": "0.1.0",
	"title": "Edwiser Orders",
	"category": "edwiser",
	"icon": "list-view",
	"description": "Display user's orders with pagination and search functionality.",
	"example": {},
	"supports": {
		"html": false
	},
	"attributes": {
		"pageTitle": {
			"type": "string",
			"default": "Orders"
		},
		"hidePageTitle": {
			"type": "boolean",
			"default": false
		},
		"enableEdwiserOrders": {
			"type": "boolean",
			"default": true
		},
		"enableWooCommerceOrders": {
			"type": "boolean",
			"default": false
		},
		"defaultTab": {
			"type": "string",
			"default": "eb-orders",
			"enum": [
				"eb-orders",
				"wc-orders"
			]
		}
	},
	"textdomain": "edwiser-bridge",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js"
}